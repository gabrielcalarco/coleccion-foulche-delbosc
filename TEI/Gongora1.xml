      <div type="poem" met="11" rhyme="abbacdeced"> 
           <head>Sonetto 1</head>
                 <lg n="1"> 
                    <l xml:id="L1">No en bronces, que caducan, mortal m<rhyme label="ano">áno</rhyme>,</l> 
                    <l xml:id="L2">Ò Cathólico sol de los Baç<rhyme label="anes">anes</rhyme>,</l>
                    <l xml:id="L3">(Que ia éntre gloriôsos Capit<rhyme label="anes">anes</rhyme></l>
                    <l xml:id="L4">Eres Deidad armada, Marte hum<rhyme label="ano">áno</rhyme>)</l>
            </lg>
                 <lg n="2"> 
                    <l xml:id="L5">Esculpirá tus hechos, sino en v<rhyme label="ano">ano</rhyme></l> 
                    <l xml:id="L6">Quando describir quiera tus aff<rhyme label="anes">anes</rhyme>,</l>
                    <l xml:id="L7">I los bien reportados tafet<rhyme label="anes">anes</rhyme></l> 
                    <l xml:id="L8">De el Turco, de el Inglés, de el Lusit<rhyme label="ano">ano</rhyme>.</l>
            </lg>
                 <lg n="3"> 
                    <l xml:id="L9">El un mar, de tus velas coron<rhyme label="ado">ado</rhyme>,</l> 
                    <l xml:id="L10">De tus remos, el otro encanec<rhyme label="ido">ido</rhyme></l> 
                    <l xml:id="L11">Tablas serán de cosas tan estr<rhyme label="añas">añas</rhyme>.</l> 
            </lg>
                 <lg n="4"> 
                    <l xml:id="L12">De la immortalidad, el no cans<rhyme label="ado">ado</rhyme></l>
                    <l xml:id="L13">Pinzel las lógre: i sean tus haç<rhyme label="añas">añas</rhyme></l>
                    <l xml:id="L14">Alma de el tiempo, escolta de el olv<rhyme label="ido">ido</rhyme>.</l>
            </lg>
        </div>
